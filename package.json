{
  "name": "idev-assistant",
  "displayName": "Idev Assistant",
  "publisher": "GG-bond-ZTW",
  "description": "A vscode extension for the requirements management tool Idev2, that helps users automatically count coding time by issue",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "icon": "assets/Idevlogo.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.idevLogout",
        "title": "idev退出登录"
      },
      {
        "command": "extension.clearWorkload",
        "title": "清除idev工作量数据"
      },
      {
        "command": "extension.setWorkloadMode",
        "title": "切换Idev工作量统计模式"
      },
      {
        "command": "idev.refresh",
        "title": "刷新视图",
        "icon": "$(refresh)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "idev.refresh",
          "when": "view == idev-assistant",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "idev-explorer",
          "title": "idev Explorer",
          "icon": "assets/Idevlogo.png"
        }
      ]
    },
    "views": {
      "idev-explorer": [
        {
          "type": "webview",
          "id": "idev-assistant",
          "name": "Idev2-assistant"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash": "^4.17.16",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zhangtianwei1998/Idev-Assistant"
  },
  "license": "MIT",
  "author": "GG-bond-ZTW"
}
