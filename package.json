{
  "name": "idev-assistant",
  "displayName": "Idev Assistant",
  "publisher": "GG-bond-ZTW",
  "description": "A hello world sample extension to demonstrate using React + Vite with the Webview UI Toolkit for VS Code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (React + Vite): Show"
      },
      {
        "command": "extension.clearIdevToken",
        "title": "Clear idevToken"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "idev-explorer",
          "title": "idev Explorer",
          "icon": "assets/Idevlogo.png"
        }
      ]
    },
    "views": {
      "idev-explorer": [
        {
          "type": "webview",
          "id": "idev-assistant",
          "name": "Idev2-assistant"
        }
      ]
    }
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.46.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  }
}
